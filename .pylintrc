[MASTER]
disable=
    format, # Handled by Black autoformatter
    duplicate-code,
    import-error,
    fixme,
    global-statement,
    import-error,
    line-too-long,
    missing-module-docstring,
    wrong-import-order,

[MESSAGES CONTROL]
disable=
    bad-continuation, # Black compatible
    bad-whitespace, # Black compatible
